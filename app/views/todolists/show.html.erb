<h2>タイトル</h2>
<p><%= @list.title %></p>
<h2>本文</h2>
<p><%= @list.body %></p>
<h2>投稿者</h2>
<p><%= @list.name %></p>
<h2>画像</h2>
<%= attachment_image_tag @list, :image, format:'jpeg', size: "300x300" %>

<%= link_to "編集", edit_todolist_path(@list.id) %>
<%= link_to "削除", todolist_path(@list.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>
<%= link_to "戻る", todolists_path %>