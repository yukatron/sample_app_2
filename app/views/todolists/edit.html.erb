<h1>編集画面</h1>
<%= form_for(@list, url:"/todolists/#{@list.id}") do |f| %>
	<h4>タイトル</h4>
	<%= f.text_field :title %>
	<h4>本文</h4>
	<%= f.text_area :body %>
	<p>投稿者名 <%= @list.name %></p>

	<%= attachment_image_tag @list, :image, :fill, 300, 300, format: 'jpeg' %>
	<%= f.attachment_field :image %>

	<%= f.submit '保存' %>
<% end %>