<h1>新規投稿</h1>

<%= form_for(@list, url:'/todolists') do |f| %>
	<h4>投稿者名</h4>
	<%= f.text_field :name %>
	<h4>タイトル</h4>
	<%= f.text_field :title %>
	<h4>本文</h4>
	<%= f.text_area :body %>
	<h4>画像</h4>
	<%= f.attachment_field :image %>
	<%= f.submit '投稿'%>
<% end %>

<%= link_to "投稿一覧", todolists_path %>